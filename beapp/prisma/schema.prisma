generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid()) @db.Uuid
  userName String? @unique
  email     String   @unique
  password  String
  avatar   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
